# ignore wp-config.php and htaccess etc.
wp-config.php
.htaccess

# ignore editor files
*~

# ignore everything in the root except the "wp-content" directory.
!wp-content/

# ignore directories except "wp-content" directory, since our own code and files are there
!wp-content/*
wp-includes/*
wp-admin/*

# ignore node dependency directories
node_modules/

# ignore log files and databases
*.log
*.sql
*.sqlite
