# ignore wp-config.php and htaccess etc.
wp-config.php
.htaccess

# ignore editor files
*~

# ignore everything in the root except the "wp-content" directory.
!wp-content/

# ignore everything in the "wp-content" directory, except our own plugin and theme
wp-content/*
wp-includes/*
wp-admin/*
!wp-content/plugins/fifm-plugin/
!wp-content/themes/fifm-theme/
!wp-content/uploads/

# ignore node dependency directories
node_modules/

# ignore log files and databases
*.log
*.sql
*.sqlite
